| {%load static%}
doctype html
html
  head
    title Add Experience
    include includes/head.pug
  body
    p: a(href="{% url 'logout' %}") Logout
    h4 {{welcomeMessage}}
    form#add-experience(method="POST")
      | {% csrf_token %}
      | {{ expAddForm.as_p }}
      button(type="submit") Save
